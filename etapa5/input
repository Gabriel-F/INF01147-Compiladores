int y;
int z;

int foo(int x, int y){
        if (x > y) then {
                y = 3;
        };

        return y;
}
int boo(int x){
        if (x > 0)then{
                x = 0;
        };
        return x;
}

int main(){
        int x;
        x = 10;
        y = 4;
        z = 2 + x / y - ( 7 * 6);

        while ( x > 5 ){
                x =  x - 1;
        };

        if(x > 0 || y > 0 ) then {
            x = 10;
        };

        //y = foo(x , y);
        x = boo(x);
}